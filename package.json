{
  "name": "ckb-wallet-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "tsc",
    "clean": "rm -rf build",
    "fmt": "prettier --write \"src/**/*.ts\" package.json .eslintrc.js knexfile.ts",
    "lint": "eslint -c .eslintrc.js \"src/**/*.ts\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon ./bin/www",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"yarn run watch-ts\" \"yarn run watch-node\""
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "knex": "^0.21.3",
    "morgan": "~1.9.1",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.6.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
